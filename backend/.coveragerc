[run]
source = .
omit =
    */venv/*
    */tests/*
    */__pycache__/*
    */site-packages/*
    app.py
    config.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
